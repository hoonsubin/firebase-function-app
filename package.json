{
    "name": "firebase-functions",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "scripts": {
        "lint": "eslint --ext .js,.ts .",
        "build": "tsc",
        "serve": "yarn run build && firebase emulators:start --only functions",
